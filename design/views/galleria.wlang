<div id="galleria">
  *{pictures}{
    <img src="+{src}"/>
  }
</div>
<script src="/js/libs/galleria-1.2.7.min.js"></script>
<script>
  $(function(){
    Galleria.loadTheme("/js/libs/galleria.classic.min.js");
    Galleria.configure({
        transition: 'fade',
        imageCrop: true
    });
    Galleria.run('#galleria');
  });
</script>
